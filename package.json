  "devDependencies": {
    "@types/babel__core": "^7.0.1",
    "@types/mime": "^2.0.0",
    "@types/node": "^10.12.0",
    "eslint": "^5.8.0",
    "prettier": "^1.16.4",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "any-cfg": "^0.9.0",
    "babel-plugin-bare-import-rewrite": "^1.0.0",
    "mime": "^2.3.1",
    "strip-ansi": "^5.1.0"
  }
}
