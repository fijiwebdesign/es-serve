{
  "name": "pwa-serve",
  "version": "0.2.1",
  "description": "Simple web server with babel compilation on the fly",
  "author": "phaux <phaux4@gmail.com>",
  "license": "ISC",
  "repository": "phaux/es-serve",
  "keywords": [
    "esnext",
    "server",
    "spa",
    "pwa",
    "babel"
  ],
  "bin": "lib/index.js",
  "main": "lib/index.js",
  "scripts": {
